{
  "name": "fire-tutorial-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.default next dev",
    "build": "env-cmd -f .env.default next build",
    "start": "env-cmd -f .env.default next start",
    "lint": "next lint && stylelint 'src/**/*.css'",
    "lint:fix": "next lint --fix && stylelint 'src/**/*.css' --fix",
    "format": "prettier --write .",
    "deploy": "rm -rf .firebase && yarn build && yarn rewrites:generate && firebase deploy --except functions --project default",
    "rewrites:generate": "next-csr-rewrites-generate --format firebase && yarn format"
  },
  "dependencies": {
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@sonicgarden/next-csr-rewrites-generator": "^1.2.0",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "autoprefixer": "latest",
    "env-cmd": "^10.1.0",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "latest",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "latest",
    "typescript": "latest"
  }
}
